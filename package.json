  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "lint:core": "ng lint @myorg/core",
    "lint:all": "yarn lint:core",
    "ngbuild:core": "ng build @myorg/core",
    "build:core": "yarn lint:core && yarn ngbuild:core",
    "build-prod:core": "yarn lint:core && yarn ngbuild:core --configuration production",
    "build:all": "yarn --yarn build:core && yarn doc:all",
    "build-prod:all": "yarn build-prod:core && yarn doc:all",
    "test:core": "ng test @myorg/core --watch=false --code-coverage",
    "test:core/sub-module": "ng test @myorg/core --watch=false --code-coverage --main=projects/myorg/core/sub-module/src/test.ts --ts-config=projects/myorg/core/sub-module/tsconfig.spec.json --karma-config=projects/myorg/core/sub-module/karma.conf.js",
    "doc:core": "cd ./projects/myorg/core && typedoc --tsconfig tsconfig.lib.json && cd ../../../",
    "doc:core-sub-module": "cd ./projects/myorg/core && typedoc --tsconfig tsconfig.lib.json --entryPoints sub-module/src/public_api.ts --out ../../../dist/myorg/core/docs/sub-module && cd ../../../",
    "doc:all": "yarn doc:core && yarn doc:core-sub-module",
    "postinstall": "ngcc"
  },
